{
  "name": "el-capitan",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run ${NAME}:serve",
    "nx": "nx",
    "generate:lib": "npm run generate:react && npm run setup-storybook && npm run copy-storybook-config && npm run setup-tailwind",
    "generate:react": "nx generate @nrwl/react:library ${NAME} --unitTestRunner=vitest --style=css --bundler=vite --compiler=swc --globalCss=true --importPath=@el-cap/${NAME} --publishable",
    "setup-tailwind": "nx generate @nrwl/react:setup-tailwind ${NAME}",
    "setup-storybook": "nx generate @nrwl/storybook:configuration ${NAME} --uiFramework=@storybook/react --bundler=vite --configureCypress=false && nx generate @nrwl/react:stories --generateCypressSpecs=false --project=${NAME}",
    "copy-storybook-config": "rm -rf packages/${NAME}/.storybook && cp -R .storybook packages/${NAME}/",
    "lint": "nx run ${NAME}:lint",
    "build": "nx run ${NAME}:build",
    "test": "nx run ${NAME}:test",
    "build-storybook": "nx run ${NAME}:build-storybook",
    "storybook": "nx run ${NAME}:storybook",
    "prepare": "husky install",
    "version": "nx run-many --targets=version --parallel=false --all --releaseAs=${TYPE}",
    "version:pre-release": "nx run workspace:version --releaseAs=prerelease --preid=${TYPE}",
    "perma-deploy": "rm -rf dist/apps/el-cap-* && node ./tools/scripts/perma-deploy.mjs dist/apps/el-cap/"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "7.12.13",
